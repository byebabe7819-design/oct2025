{
  "hosting": {
    "public": "public",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "headers": [
      {"source": "**/*", "headers":[{"key":"X-Content-Type-Options","value":"nosniff"}]},
      {"source": "/**", "headers":[{"key":"Cache-Control","value":"public, max-age=300"}]}
    ],
    "rewrites": [{ "source": "/api/**", "function": "api" }]
  }
}
